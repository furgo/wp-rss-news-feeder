{
    "target_directory": "src/Libs",
    "namespace_prefix": "Furgo\\RssNewsFeeder\\Libs\\",
    "classmap_prefix": "FUR_",
    "constant_prefix": "FUR_",
    "packages": [
        "php-di/php-di",
        "psr/container",
        "psr/log"
    ],
    "exclude_from_prefix": {
        "namespaces": [
            "Furgo\\Sitechips\\Core\\"
        ],
        "file_patterns": [
            "*/lib/*"
        ]
    },
    "delete_vendor_files": false,
    "delete_vendor_packages": true,
    "update_call_sites": true,
    "override_autoload": []
}